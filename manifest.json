{
    "manifest_version": 2,
    "name": "Laravel MCQ",
    "version": "1.0",
    "description": "Laravel MCQ",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "index.html"
    },
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "scripting",
      "http://*/*",
      "https://*/*",
      "storage"
    ]
  }